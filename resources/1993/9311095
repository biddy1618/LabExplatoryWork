\put(5,15){\line(2,-3){10}}
\put(15,15){\line(-2,-3){4}}
\put(5,0){\line(2,3){4}}
}} ,\\
&\ev:X\tens X\pti \to k &&\qquad \text{by} \qquad
%xxptiev.pic
\unitlength=1mm
