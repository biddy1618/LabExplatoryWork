\begin{eqnarray*}
& & \cos m_0=\sqrt{S^E_2 \over S^E_4}; \qquad \cos m_1=\sqrt{S^E_2 \over
S^E_3}; \qquad \cos m_2=\sqrt{S^E_5 \over S^E_4};\\
& & \cos m_3=\sqrt{S^E_6 \over S^E_5}; \qquad
\cos m_4=\sqrt{S^E_7 \over S^E_6}; \qquad \sin m_5=\sqrt{S^E_6 \over S^E_5}; \\
& & \sin m_6=\sqrt{S^E_5 \over S^E_4}; \qquad \sin m_7=\sqrt{S^E_2 \over
S^E_3}; \qquad \cos m_8=\sqrt{S^E_1 \over S^E_2}; \\
& & \sin m_9=\sqrt{S^E_8 \over S^E_4};
\end{eqnarray*}
