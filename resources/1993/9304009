\put(25,25){\scriptsize 5} \put(1,25){\scriptsize 11}
\put(30,14){=}
\multiput(14,1)(0,25){2}{\scriptsize 1}
\end{picture}
\begin{picture}(40,30)(-5,0)
\multiput(5,5)(0,20){2}{\vector(1,0){10}}
\multiput(25,5)(0,20){2}{\line(-1,0){10}}
\multiput(5,5)(20,0){2}{\line(0,1){10}}
\multiput(5,25)(20,0){2}{\vector(0,-1){10}}
\put(0,0){\scriptsize 13} \put(25,2){\scriptsize 3}
\put(25,25){\scriptsize 4} \put(1,25){\scriptsize 14}
\put(30,14){=}
\multiput(14,1)(0,25){2}{\scriptsize 1}
\end{picture}
\begin{picture}(40,30)(-5,0)
\multiput(5,5)(0,20){2}{\vector(1,0){10}}
\multiput(25,5)(0,20){2}{\line(-1,0){10}}
\multiput(5,5)(20,0){2}{\line(0,1){10}}
\multiput(5,25)(20,0){2}{\vector(0,-1){10}}
\put(0,0){\scriptsize 13} \put(25,2){\scriptsize 7}
\put(25,25){\scriptsize 6} \put(1,25){\scriptsize 14}
\put(30,14){$=1$;}
\multiput(14,1)(0,25){2}{\scriptsize 1} \end{picture} \\ %& & \\
& & \begin{picture}(40,30)(-5,0)
\multiput(5,5)(0,20){2}{\vector(1,0){10}}
\multiput(25,5)(0,20){2}{\line(-1,0){10}}
\multiput(5,5)(20,0){2}{\line(0,1){10}}
\multiput(5,25)(20,0){2}{\vector(0,-1){10}}
\put(0,0){\scriptsize 13} \put(25,2){\scriptsize 7}
\put(25,25){\scriptsize 6} \put(1,25){\scriptsize 12}
\put(30,14){=}
\multiput(14,1)(0,25){2}{\scriptsize 1}
\end{picture}
\begin{picture}(40,30)(-5,0)
\multiput(5,5)(0,20){2}{\vector(1,0){10}}
\multiput(25,5)(0,20){2}{\line(-1,0){10}}
\multiput(5,5)(20,0){2}{\line(0,1){10}}
\multiput(5,25)(20,0){2}{\vector(0,-1){10}}
\put(0,0){\scriptsize 6} \put(25,2){\scriptsize 6}
\put(25,25){\scriptsize 5} \put(1,25){\scriptsize 5}
\put(30,14){$=-1$;}
\multiput(14,1)(0,25){2}{\scriptsize 1}
\end{picture} \\ %& & \\
& & \begin{picture}(40,30)(-5,0)
\multiput(5,5)(0,20){2}{\vector(1,0){10}}
\multiput(25,5)(0,20){2}{\line(-1,0){10}}
\multiput(5,5)(20,0){2}{\line(0,1){10}}
\multiput(5,25)(20,0){2}{\vector(0,-1){10}}
\put(1,2){\scriptsize $a$} \put(25,2){\scriptsize $b$}
\put(25,25){\scriptsize $c$} \put(1,25){\scriptsize $d$}
\put(30,14){=}
\multiput(14,1)(0,25){2}{\scriptsize 1}
\end{picture}
\begin{picture}(60,30)(-5,0)
\multiput(15,5)(0,20){2}{\vector(1,0){10}}
\multiput(35,5)(0,20){2}{\line(-1,0){10}}
\multiput(15,5)(20,0){2}{\line(0,1){10}}
\multiput(15,25)(20,0){2}{\vector(0,-1){10}}
\put(-4,2){\scriptsize $30\!\!-\!\!a$} \put(35,2){\scriptsize $b$}
\put(35,25){\scriptsize $c$} \put(-4,25){\scriptsize $30\!\!-\!\!d$}
\multiput(24,1)(0,25){2}{\scriptsize 1} \put(55,14){for $a>15$ and $d>15$;}
 \end{picture}
\end{eqnarray*}
