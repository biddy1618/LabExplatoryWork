\begin{eqnarray*}
& & \sin m_1 = {\sqrt{{{{S^A_7}\,{S^E_6}}\over {{S^A_5}\,{S^E_4}}}}};\;\;
\;\;\;\;
 \sin m_2 = {\sqrt{{{{S^A_4}\,{S^E_7}}\over {{S^A_6}\,{S^E_5}}}}};\\
& & \cos m_3 = {\sqrt{{{{S^A_5}\,{S^E_6}}\over {{S^A_7}\,{S^E_4}}}}};\;\;
\;\;\;\;
 \cos m_4 = {\sqrt{{{{S^A_{10}}\,{S^E_1}} \over {{S^A_8}\,{S^E_3}}}}};\\
&	& \sin m_5 = {\sqrt{{{{S^A_7}\,{S^E_2}}\over {{S^A_9}\,{S^E_4}}}}};
\;\;\;\;\;\;
	\cos m_6 = {\sqrt{{{{S^A_5}\,{S^E_6}}\over {{S^A_9}\,{S^E_2}}}}};\\
& & \cos m_7 = {\sqrt{{{{S^A_8}\,{S^E_7}}\over {{S^A_{10}}\,{S^E_3}}}}}.
\end{eqnarray*}
