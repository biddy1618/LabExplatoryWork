\begin{eqnarray*}
& & \sin m_1=\sqrt{S^E_1 \over S^E_2};\qquad \sin m_2=\sqrt{S^E_5 \over S^E_3};
\qquad \sin m_3=\sqrt{S^E_5 \over S^E_4};\\
& & \cos m_4=\sqrt{S^E_7 \over S^E_3}; \qquad
\sin m_5=\sqrt{S^E_7 \over S^E_6};\qquad \sin m_6=\sqrt{S^E_1 \over S^E_3};
\end{eqnarray*}
