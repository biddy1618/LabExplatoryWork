\vspace{-.1in}
\begin{eqnarray*}
&	& a_1=\sin\gamma; \\
&	& a_2=\cos\gamma\,\sin\alpha; \\
&	& a_3=\cos\alpha\,\cos\gamma; \\
&	& b_1=\cos\gamma\,\sin\beta; \\
&	& b_2=\cos\alpha\,\cos\beta -
\sin\alpha\,\sin\beta\,\sin\gamma; \\
&	& b_3= -\cos\beta\,\sin\alpha  -
\cos\alpha\,\sin\beta\,\sin\gamma; \\
&	& c_1=\cos\beta\,\cos\gamma; \\
&	& c_2=-\cos\alpha\,\sin\beta -
 \cos\beta\,\sin\alpha\,\sin\gamma; \\
&	& c_3=\sin\alpha\,\sin\beta -
	\cos\alpha\,\cos\beta\,\sin\gamma; \\
&	& \aa_1=\sin m_{3}=a_1\,\sqrt{{{S^E_6}}\over {{S^E_5}}}; \\
&	& \aa_3= a_3\,\sqrt{{{{S^E_6}\over {{S^E_9}}}}}; \;\;\;\;\;\;\;
\aa_2=\cos m_{6}=a_2\,\sqrt{{{S^E_6}   \over {{S^E_7}}}}; \\
&	& \bb_1=b_1\,\sqrt{{{S^E_6}\over  {{S^E_5}}}}; \;\;\;\;\;\;\;
\bb_2= b_2\,\sqrt{{{S^E_6}\,   \over {{S^E_7}}}}; \\
&	& \bb_3= b_3\,\sqrt{{{S^E_6} \over {{S^E_9}}}}; \;\;\;\;\;\;\;
\cc_1=c_1\,\sqrt{{{S^E_6}\over  {{S^E_5}}}}; \\
&	& \cc_2= c_2\,\sqrt{{{S^E_6}\,   \over {{S^E_7}}}}; \;\;\;\;\;\;\;
\cc_3=c_3\,\sqrt{{{S^E_6} \over {{S^E_9}}}}; \\
&	& \cos\gamma={\sqrt{{{{S^E_5}}\over   {{S^E_6}}}}}; \;\;\;\;\;\;\;
\sin\alpha=\sin\beta={\sqrt{{{{S^E_6}-{S^E_7}}\over
			{{S^E_5}}}}}; \\
&	& \sin m_1=\sqrt{S^E_1 \over S^E_3}; \qquad
\cos m_2=\sqrt{S^E_3 \over S^E_4}; \qquad
\cos m_3=\sqrt{S^E_4 \over S^E_5}; \\
&	&  \cos m_4=\sqrt{S^E_3 \over S^E_5}; \qquad
\sin m_5=\sqrt{S^E_8 \over S^E_6}; \qquad
\sin m_6=\sqrt{S^E_8 \over S^E_7}; \\
& & \cos m_7=\sqrt{S^E_1 \over S^E_2}; \qquad
\sin m_8=\sqrt{S^E_2 \over S^E_4}; \qquad \cos m_9=\sqrt{S^E_9 \over S^E_6};
\end{eqnarray*}
