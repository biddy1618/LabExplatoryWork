\right) \;\;$ = }
\multiput(39,61)(40,0){3}{\scriptsize 1}
\multiput(39,35)(40,0){3}{\scriptsize 1}
\multiput(39,21)(40,0){3}{\scriptsize 1}
\multiput(39,-5)(40,0){3}{\scriptsize 1}
\multiput(39,-19)(40,0){3}{\scriptsize 1}
\multiput(39,-45)(40,0){3}{\scriptsize 2}
\put(165,10){$\left( \begin{array}{ccc}	a_3 & a_2 & a_1 \\  \\
	-b_3 & -b_2 & -b_1   \\  \\
	c_3 & c_2 & c_1   \end{array} \right)  \; ; $}
