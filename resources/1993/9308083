\vskip.3truein
\centerline{%
      {\csc Philip~Candelas}$^{1,2}$,\quad
      {\csc Xenia~de la Ossa}$^3$,\quad
      {\csc Anamar\'\i a~Font}$^4$,}
\vskip1mm
\centerline{{\csc Sheldon Katz}$^5$ {\csc and David R.~Morrison}$^{6,7}$}
\vskip.2truein\bigskip
%
\centerline{
