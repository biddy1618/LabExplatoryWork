\def\figBflat{{\twelvecal B}\raise5pt\hbox{$\twelvemath\flat$}}

\def\gcheck{\hbox{\lower2.5pt\hbox{\Bigcheck}\kern-8pt$\g$}}
\def\lhat{\hbox{\raise.5pt\hbox{\Bighat}\kern-8pt$\l$}}
