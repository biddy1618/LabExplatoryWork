\def\matrixa{\pmatrix{\- 0 &\- 1 &\- 0 &\- 0 &\- 0 &\- 0 \cr
                      \- 0 &\- 0 &\- 1 &\- 0 &\- 0 &\- 0 \cr
                      \- 0 &\- 0 &\- 0 &\- 1 &\- 0 &\- 0 \cr
                      \- 0 &\- 0 &\- 0 &\- 0 &\- 1 &\- 0 \cr
                      \- 0 &\- 0 &\- 0 &\- 0 &\- 0 &\- 1 \cr
                        -1 &\- 0 &  -1 &\- 0 &  -1 &\- 0 \cr  }}

\def\matrixb{\pmatrix{\- 1 &\- 0 &\- 0 &\- 0 &\- 0 &\- 0 \cr
                      \- 0 &\- 1 &  -1 &\- 1 &  -1 &\- 1 \cr
                      \- 0 &\- 0 &\- 2 &  -1 &\- 1 &  -1 \cr
                      \- 3 &  -3 &\- 4 &  -3 &\- 3 &  -3 \cr
                        -3 &\- 3 &  -4 &\- 4 &  -2 &\- 3 \cr
                        -3 &\- 3 &  -3 &\- 3 &  -2 &\- 3 \cr  }}

\def\matrixm{\pmatrix{\- 1 &\- 0 &\- 0 &\- 0 &\- 0 &\- 0 \cropen{3pt}
-1 &\- 0&\- {1\over 2}& -{1\over 2}&\- {1\over 2}& -{1\over2} \cropen{3pt}
\- {1\over 4}&\- {3\over 4}&\- 1&\- 0&\- {1\over 4}& -{1\over 4} \cropen{3pt}
                      \- 1 &  -1 &\- 0 &\- 0 &\- 0 &\- 0 \cropen{3pt}
\- {3\over 4}& -{3\over 4}&\- 0&\- 0& -{1\over 4}&\- {1\over 4} \cropen{3pt}
\- 0&\- {3\over 2}&\- {3\over 2}&\- {1\over 2}&\- {1\over 2}&\- 0\cropen{3pt}}}

\def\matrixt{\pmatrix{\- 2 &  -1 &\- 0 &\- 0 &\- 0 &\- 0 \cr
                      \- 1 &\- 0 &\- 0 &\- 0 &\- 0 &\- 0 \cr
                        -1 &\- 1 &\- 1 &\- 0 &\- 0 &\- 0 \cr
                        -3 &\- 3 &\- 0 &\- 1 &\- 0 &\- 0 \cr
                      \- 3 &  -3 &\- 0 &\- 0 &\- 1 &\- 0 \cr
                      \- 3 &  -3 &\- 0 &\- 0 &\- 0 &\- 1 \cr  }}

\def\matrixA{\pmatrix{\- 1 &\- 0 &\- 0 &  -1 &\- 0 &\- 0 \cr
                        -1 &\- 0 &  -1 &\- 1 &\- 1 &\- 1 \cr
                        -2 &  -1 &\- 0 &\- 2 &  -1 &\- 1 \cr
                      \- 2 &\- 0 &  -1 &  -1 &  -1 &\- 0 \cr
                      \- 0 &  -1 &\- 1 &\- 0 &  -2 &  -1 \cr
                        -3 &  -1 &  -1 &\- 3 &  -1 &\- 2 \cr  }}

\def\matrixB{\pmatrix{\- 1 &\- 0 &\- 0 &\- 0 &\- 0 &\- 0 \cr
                        -4 &  -3 &\- 0 &\- 0 &  -2 &\- 0 \cr
                      \- 0 &\- 0 &\- 1 &\- 0 &\- 0 &\- 0 \cr
                      \- 2 &\- 2 &\- 0 &\- 1 &\- 0 &\- 0 \cr
                      \- 2 &\- 2 &\- 0 &\- 0 &\- 1 &\- 0 \cr
                      \- 0 &\- 0 &\- 0 &\- 0 &\- 0 &\- 1 \cr }}

\def\matrixT{\pmatrix{\- 1 &\- 0 &\- 0 &\- 1 &\- 0 &\- 0 \cr
                      \- 0 &\- 1 &\- 0 &\- 0 &\- 0 &\- 0 \cr
                      \- 0 &\- 0 &\- 1 &\- 0 &\- 0 &\- 0 \cr
                      \- 0 &\- 0 &\- 0 &\- 1 &\- 0 &\- 0 \cr
                      \- 0 &\- 0 &\- 0 &\- 0 &\- 1 &\- 0 \cr
                      \- 0 &\- 0 &\- 0 &\- 0 &\- 0 &\- 1 \cr }}

\def\matrixtinf{\pmatrix{\- 1&\- 0&\- 0&\- 0&\- 0&\- 0\cr
                         \- 2&\- 1&\- 0&\- 1&\- 0&\- 1\cr
                           -1&\- 0&\- 0&  -1&\- 0&  -1\cr
                           -3&  -3&\- 1&  -3&\- 0&  -3\cr
                         \- 3&\- 3&\- 0&\- 4&\- 0&\- 3\cr
                         \- 3&\- 3&\- 0&\- 3&\- 1&\- 3\cr}}

\def\matrixTinf{\pmatrix{\- 1&\- 0&\- 0&\- 0&\- 0&\- 0\cr
                           -1&  -2&  -1&\- 0&  -1&\- 1\cr
                         \- 0&  -1&\- 2&\- 0&  -1&  -1\cr
