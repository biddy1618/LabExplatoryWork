%
%% FOLLOWING LINE CANNOT BE BROKEN BEFORE 80 CHAR
%%\PSGraphic{filename(full)}{height,cm}{width,cm}{xoff,cm}{yoff,cm}{scalefactor}
\def\PSGraphic#1#2#3#4#5#6{%
  \vskip 10pt \nobreak%
  \hbox to \hsize{%
    \hss%
    \dimen0 = #3truecm%
    \hbox to  #6\dimen0{%
    \dimen0 = #2truecm%
    \vbox to  #6\dimen0{%
                       \vss%
                       \special{ps::[asis, begin]
                                0 SPB
                                /ChartCheckPoint save def
                                  /showpage {} def
                                  Xpos Ypos translate
                                  #6 dup scale
                                  #4 72 mul neg #5 72 mul neg translate
                               }
                       \special{ps: plotfile #1 asis}
                       \special{ps::[asis, end]
                                ChartCheckPoint restore
                                0 SPE
                               }%
                      }%
    \hss%
    }%
  \hss%
  }%
  \vskip 10pt%
}

