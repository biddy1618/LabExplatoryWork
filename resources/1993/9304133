             & 8 & \arraycent { ( \jc, 1 \csp J, J, 1, 0)  \\[.3 mm]
                 ( \js, 1 \csp J, 1, J, 0) \\[.3 mm]
                %  ( \jv, 1 \csp 1, J, J, 0)    \\[.3 mm]
                    (1, \jv \csp J, J, J, \pm (k+6)) }{1}
                 & \arraycent{+\\[.3 mm] +\\[.3 mm] -}{1} \\[-3.2 mm] &&&\\
