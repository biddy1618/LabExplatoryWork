\def\umemag{(1 \mp{\gamma_{5}})}
\def\l2{\lambda^{2}}
\def\al{\alpha}
\def\fcab{f^{c}_{ab}}
\def\etab{\eta_{ab}}
\def\meio{\frac{1}{2}}
\def\dik{\delta_{ik}}
\def\djl{\delta_{jl}}
\def\dxy{\delta(x_{1}-y_{1})}
\def\umeal{(1-\alpha)}
