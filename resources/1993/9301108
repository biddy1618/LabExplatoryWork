\def\ilmema{i_{\mp}^{L}}
\def\jprma{{j^{\psi}}_{+}^{R}}
\def\jprme{{j^{\psi}}_{-}^{R}}
\def\jprmame{{j^{\psi}}_{\pm}^{R}}
\def\jprmema{{j^{\psi}}_{\mp}^{R}}
\def\jplma{{j^{\psi}}_{+}^{L}}
\def\jplme{{j^{\psi}}_{-}^{L}}
\def\jplmame{{j^{\psi}}_{\pm}^{L}}
\def\jplmema{{j^{\psi}}_{\mp}^{L}}
\def\klma{K^{L}_{+}}
\def\klme{K^{L}_{-}}
\def\klmame{K^{L}_{\pm}}
\def\klmema{K^{L}_{\mp}}
\def\krma{K^{R}_{+}}
\def\krme{K^{R}_{-}}
\def\krmame{K^{R}_{\pm}}
\def\krmema{K^{R}_{\mp}}
\def\ylma{Y^{L}_{+}}
\def\ylme{Y^{L}_{-}}
\def\ylmame{Y^{L}_{\pm}}
\def\ylmema{Y^{L}_{\mp}}
\def\yrma{Y^{R}_{+}}
\def\yrme{Y^{R}_{-}}
\def\yrmame{Y^{R}_{\pm}}
\def\yrmema{Y^{R}_{\mp}}
