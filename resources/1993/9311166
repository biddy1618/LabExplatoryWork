        {\rlap{$\vert$}}\!\rightarrow}
%This one goes from one item bentarrow to second one.

\def\onedk#1#2{
\begin{equation}
\begin{array}{l}
 #1 \\
 \bentarrow #2
\end{array}
\end{equation}
}
%This one has five options {#1} arrow {#2}{#3} bentarrow {#4}
%bentarrow{#5}

