\begin{equation}
\begin{array}{r c l}
#1 & \rightarrow & #2#3 \\
 & & \:\raisebox{1.3ex}{\rlap{$\vert$}}\raisebox{-0.5ex}{$\vert$}%
\phantom{#2}\!\bentarrow #4 \\
 & & \bentarrow #5
\end{array}
\end{equation}
}
