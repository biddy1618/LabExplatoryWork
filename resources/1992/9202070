\def\fone{			% B_3^1
\dynkind3
\put(20,0){\spot}
\put(11,1){\dit}
\put(11,-1){\dit}
\put(13,-5){\line(1,1){5}}
\put(13,5){\line(1,-1){5}}
}

\def\ftwo{			% A_5^2
\dynkind3
\put(20,0){\spot}
\put(11,1){\dit}
\put(11,-1){\dit}
\put(12,0){\line(1,1){5}}
\put(12,0){\line(1,-1){5}}
}

