\put(0,0){\spot}
\put(10,0){\spot}
\put(2,5){\line(1,-1){5}}
\put(2,-5){\line(1,1){5}}
\put(1,0){\dit}
\put(1,1){\dit}
\put(1,-1){\dit}
}

