\centerline{The exterior derivative of $T_i$}
$$
d\,T_{1} = {{( -{q^2} + s ) }\over
{-q + {q^3}}}\,T_{1}\,\omega ^{1} - s\,T_{2}\,
\omega ^{2} - s\,T_{3}\,\omega ^{3} +
    {{( -1 + s ) }\over {-\qm + q}}\,T_{1}\,(\omega ^{5} +
    \omega^{9})
$$
$$
d\,T_{2} = {{( -{q^2} + s -
{q^2}\,s + {q^4}\,s )}\over {-q + {q^3}}} \,T_{2}\,
\omega ^{1} - s\,T_{1}\,\omega ^{4} +
    {{( -{q^2} + s )}\over {-q + {q^3}}} \,T_{2}\,\omega ^{5} -
    s\,T_{3}\,\omega ^{6} + {{( -1 + s ) }\over
        {-\qm + q}} \,T_{2}\,\omega ^{9}
$$
$$
d\,T_{3} =  {{( -{q^2} + s - {q^2}\,s + {q^4}\,s ) }\over {-q + {q^3}}}
\,T_{3}\,(\omega ^{1} +\om^5)
 - s\,T_{1}\,\omega ^{7} -
    s\,T_{2}\,\omega ^{8} + {{( -{q^2} + s ) }\over {-q + {q^3}}}
    \,T_{3}\,\omega ^{9}
$$
$$
d\,T_{4} = {{( -{q^2} + s ) }\over
      {-q + {q^3}}}\,T_{4}\,\omega^1 - s\,T_{5}\,
\omega ^{2} - s\,T_{6}\,\omega ^{3} +
    {{( -1 + s ) }\over {-\qm + q}} \,T_{4}\,(\omega ^{5}+\om^9)
$$
$$
d\,T_{5} =  {{( -{q^2} + s - {q^2}\,s + {q^4}\,s )}
\over {-q + {q^3}}} \,T_{5}\, \omega ^{1}- s\,T_{4}\,\omega ^{4} +
    {{( -{q^2} + s ) }\over {-q + {q^3}}}\,T_{5}\,\omega ^{5} -
    s\,T_{6}\,\omega ^{6} + {{( -1 + s )}\over
        {-\qm + q}} \,T_{5}\,\omega ^{9}
$$
$$
d\,T_{6} =  {{( -{q^2} + s - {q^2}\,s + {q^4}\,s ) }\over {-q + {q^3}}}
\,T_{6}\,(\omega ^{1}+\om^5)
- s\,T_{4}\,\omega ^{7} -
    s\,T_{5}\,\omega ^{8} +
{{( -{q^2} + s ) }\over {-q + {q^3}}} \,T_{6}\,\omega ^{9}
$$
$$
d\,T_{7} =  {{( -{q^2} + s ) }\over
      {-q + {q^3}}}\,T_{7}\,
\omega ^{1} - s\,T_{8}\,\omega ^{2} - s\,T_{9}\,\omega ^{3} +
    {{( -1 + s ) }\over {-\qm + q}} \,T_{7}\,(\omega ^{5}+\om^9)
$$
$$
d\,T_{8} =  {{( -{q^2} + s -
{q^2}\,s + {q^4}\,s ) }\over {-q + {q^3}}} \,T_{8}\, \omega ^{1}
- s\,T_{7}\,\omega ^{4} +
    {{( -{q^2} + s ) }\over {-q + {q^3}}}\,T_{8}\,\omega ^{5} -
    s\,T_{9}\,\omega ^{6} + {{( -1 + s ) }\over
        {-\qm + q}}\,T_{8}\,\omega ^{9}
$$
$$
