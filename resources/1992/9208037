\[
\begin{array}{l}
%%% this is -(q^2)1211 %%%
\1\2-\2\1+(1-q^2)(\2\5+\3\8)=q\2\\[.1em]
%%% this has been obtained as (1-q^2)1223+1113 %%%
\1\3-\3\1+(1-q^2)(q^2\9\3+\2\6)=q^3\3\\[.1em]
\1\4-\4\1-(1-q^2)(\5\4+\6\7)=-q\4\\[.1em]
\1\5-\5\1=0\\[.1em]
\1\6-\6\1=0\\[.1em]
\1\7-\7\1-(1-q^2)(\8\4+\9\7)=-q\7\\[.1em]
\1\8-\8\1=0\\[.1em]
\1\9-\9\1=0\\[.1em]
\2\3-q^{-1}\3\2=0\\[.1em]
%%%% this is the result of formula 1221+(1-q^2)2332 %%%
\2\4-\4\2+(1-q^2)[\5\1-\5\5+
\7\3-q^2\8\6-(1-q^2)(\9\1-\9\5)]~~~~\\[-0.1em]
{}~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
{}~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~=q^3(\1-\5)\\[-0.45em]
\2\5-q^2\5\2+(1-q^2)\3\8=q\2\\[.1em]
%%% 1223+(1-q^2)1333 %%%
\2\6-q\6\2+(1-q^2)\3\9=q\3\\[.1em]
%%% note that 1231-2232 is more complicated than 1231 %%%
\2\7-q^{-1}\7\2+(q^{-1}-q)(\8\1-\8\5-\9\8)=-\8\\[.1em]
\2\8-q\8\2=0\\[.1em]
\2\9-\9\2=0\\[.1em]
\3\4-q^{-1}\4\3+(q^{-1}-q)(\6\1-\5\6-\6\9)=-\6\\[.1em]
\3\5-\5\3+(q^{-1}-q)\6\2=0\\[.1em]
\3\6-q\6\3=0\\[.1em]
\3\7-\7\3-(1-q^2)(\8\6-\9\1+\9\9)=q(\1-\9)\\[.1em]
\3\8-q\8\3+(1-q^2)\9\2=q\2\\[.1em]
\3\9-q^2\9\3=q\3\\[.1em]
\4\5-q^{-2}\5\4+(1-q^{-2})\6\7=-q^{-1}\4\\[.1em]
\4\6-q^{-1}\6\4=0\\[.1em]
\4\7-q\7\4=0\\[.1em]
\4\8-q^{-1}\8\4-(q^{-1}-q)\9\7=-\7\\[.1em]
\4\9-\9\4=0\\[.1em]
%%%this comes from 2223 and 2333 %%%
\5\6-\6\5+(1-q^2)\6\9=q\6\\[.1em]
%%%this comes from 2231 and 3133 %%%
\5\7-\7\5+(q^{-1}-q)\4\8=0\\[.1em]
\5\8-\8\5-(1-q^2)\9\8=-q\8\\[.1em]
\5\9-\9\5=0\\[.1em]
\6\7-q\7\6+(1-q^2)\9\4=q\4\\[.1em]
\6\8-\8\6+(1-q^2)(\9\5-\9\9)=q(\5-\9)\\[.1em]
\6\9-q^2\9\6=q\6\\[.1em]
\7\8-q^{-1}\8\7=0\\[.1em]
\7\9-q^{-2}\9\7=q^{-1}\7\\[.1em]
\8\9-q^{-2}\9\8=-q^{-1}\8
\end{array}
\]
