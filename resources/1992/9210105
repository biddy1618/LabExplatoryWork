\scriptfont\cmbsyfam=\tencmbsy \scriptscriptfont\cmbsyfam=\tencmbsy
\scriptfont\cmcscfam=\tencmcsc \scriptscriptfont\cmcscfam=\tencmcsc
\def\cmcsc{\fam\cmcscfam\tencmcsc} \textfont\cmcscfam=\tencmcsc \fi
