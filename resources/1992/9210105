\def\chapterfont#1{\xdef\ttaarr{#1}}
\def\sectionfont#1{\xdef\ppaarr{#1}}
\def\ttaarr{\bf}		% chapter titles' font
\def\ppaarr{\sl}		% section titles' font
\def\drftfont{\rm}
