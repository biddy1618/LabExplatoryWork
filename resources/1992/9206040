\font\twelvesy=cmsy10 at 12pt               \skewchar\twelvesy='60
\font\ninesy=cmsy9                          \skewchar\ninesy='60
\font\sixsy=cmsy6                           \skewchar\sixsy='60

\font\seventeenex=cmex10 scaled\magstep3
