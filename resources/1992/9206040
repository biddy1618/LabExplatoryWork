\def\mapright#1{\smash{
    \mathop{\tto}\limits^{#1}}}
\def\mapleft#1{\smash{
    \mathop{\longleftarrow}\limits^{#1}}}
\def\mapdown#1{\Big\downarrow
  \rlap{$\vcenter{\hbox{$\scriptstyle#1$}}$}}
\def\mapup#1{\Big\uparrow
  \rlap{$\vcenter{\hbox{$\scriptstyle#1$}}$}}
\def\mapsw#1{\swarrow
\rlap{$\vcenter{\hbox{$\scriptstyle#1$}}$}}
