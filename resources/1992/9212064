\def\makefootnote@#1#2{\insert\footins{\interlinepenalty=100
  \splittopskip=\ht\strutbox \splitmaxdepth=\dp\strutbox
  \floatingpenalty=\@MM
  \leftskip=\z@\rightskip=\z@\spaceskip=\z@\xspaceskip=\z@
  \noindent{#1}\footstrut\rm\ignorespaces #2\strut}}
