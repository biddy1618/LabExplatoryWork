\def\ralign@#1\endalign{\displ@y\Let@\tabskip\centering\halign to\displaywidth
     {\hfil$\displaystyle{##}$\tabskip=\z@&$\displaystyle{{}##}$\hfil
       \tabskip=\centering&\llap{\hbox{(\rm##\unskip)}}\tabskip\z@\crcr
             #1\crcr}}
