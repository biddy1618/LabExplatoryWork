\def\dots{\relax\ifmmode\let\next=\ldots\else\let\next=\tdots@\fi\next}
\def\tdots@{\unskip\ \tdots@@}
\def\tdots@@{\futurelet\next\tdots@@@}
\def\tdots@@@{$\mathinner{\ldotp\ldotp\ldotp}\,
   \ifx\next,$\else
