     #1{\leftarrow}\mkern-6mu\cleaders\hbox{$#1\mkern-2mu{-}\mkern-2mu$}\hfill
       \mkern-6mu{\to}\cr
    \noalign{\kern -1\p@\nointerlineskip}
      \hfil#1#2\hfil\cr}}}
\def\underrightarrow{\mathpalette\underrightarrow@}
