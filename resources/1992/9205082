                    \multiput(0,3)(1,0){#1}{\line(1,0){1}}
                    \multiput(1,3)(1,0){#1}{\line(0,1){1}}
                    \put (0.3,3.3){#4}
                    \put (1.3,3.3){#5}
                    \put (2.3,3.3){#6}
                    \multiput(0,2)(1,0){#2}{\line(1,0){1}}
                    \multiput(1,2)(1,0){#2}{\line(0,1){1}}
                    \multiput(0.3,2.3)(1,0){#2}{#7}
                          \put (0,4){\line(0,-1){#3}}}

\def\dmultab#1#2#3#4#5#6#7{\put (0,4){\line(-1,0){#1}}
                    \multiput(0,3)(-1,0){#1}{\line(-1,0){1}}
                    \multiput(-1,3)(-1,0){#1}{\line(0,1){1}}
                    \multiput(-0.5,3.5)(-1,0){#1}{\circle*{0.1}}
                    \multiput(0,2)(-1,0){#2}{\line(-1,0){1}}
                    \multiput(-1,2)(-1,0){#2}{\line(0,1){1}}
                    \multiput(-0.5,2.5)(-1,0){#2}{\circle*{0.1}}
                    \multiput(0,1)(-1,0){#3}{\line(-1,0){1}}
                    \multiput(-1,1)(-1,0){#3}{\line(0,1){1}}
                    \multiput(-0.5,1.5)(-1,0){#3}{\circle*{0.1}}
                    \multiput(0,0)(-1,0){#4}{\line(-1,0){1}}
                    \multiput(-1,0)(-1,0){#4}{\line(0,1){1}}
                    \multiput(-0.5,0.5)(-1,0){#4}{\circle*{0.1}}
                    \multiput(0,-1)(-1,0){#5}{\line(-1,0){1}}
                    \multiput(-1,-1)(-1,0){#5}{\line(0,1){1}}
                    \multiput(-0.5,-0.5)(-1,0){#5}{\circle*{0.1}}
                    \multiput(0,-2)(-1,0){#6}{\line(-1,0){1}}
                    \multiput(-1,-2)(-1,0){#6}{\line(0,1){1}}
                    \multiput(-0.5,-1.5)(-1,0){#6}{\circle*{0.1}}
                          \put (0,4){\line(0,-1){#7}}}
