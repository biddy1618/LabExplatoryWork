\setlength{\textwidth}{15.0cm}
\setlength{\oddsidemargin}{1.0cm}
\setlength{\evensidemargin}{1.0cm}
\setlength{\textheight}{22.0cm}
\setlength{\topmargin}{-0.3in}
\renewcommand{\theequation}{\thesection.\arabic{equation}}
\renewcommand{\thefootnote}{\fnsymbol{footnote}}
\def\onward{\addtocounter{section}{1} \setcounter{equation}{0} }

%figure macros
\def\tetra#1#2#3#4#5#6{\setlength{\unitlength}{1.0cm}
\begin{picture}(2,2.0)(1.8,2.5)
\put (2.4,2.5){$#1$}
\put (3.9,2.82){$#2$}
\put (3.5,0.77){$#3$}
\put (3.1,1.75){$#4$}
\put (3.5,4.1){$#5$}
\put (3.1,3.22){$#6$}
%%\put (0,0){\special{include data$1:[hriggs.tetra]papstetra.ps}}
%\put (0,0){\epsffile{artetra.ps}}
\end{picture}
{}~~~~~~~~~}

\def\cpstetra#1#2#3#4#5#6{\setlength{\unitlength}{1.0cm}
\begin{picture}(2,2.0)(1.8,2.5)
\put (2.4,2.5){$#1$}
\put (3.9,2.82){$#2$}
\put (3.5,0.77){$#3$}
\put (3.1,1.75){$#4$}
\put (3.5,4.1){$#5$}
\put (3.1,3.22){$#6$}
\put (3.6,2.2){${\sct \cK^{#2 #4}_{#6}}$}
\put (2.1,3.7){${\sct \cK^{#5 #6}_{#1}}$}
\put (2.1,1.4){${\sct \cK_{#3 #4}^{#1}}$}
%\put (0,0){\special{include data$1:[hriggs.tetra]papstetra.ps}}
\end{picture}
{}~~~~~~~~~}

