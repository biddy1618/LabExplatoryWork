0&(a(\l{}))^L&0\cr C_1(\l{})&C_2(\l{})&1}\right)\vac\qquad .}

\equation{state1}{|\l{1},\ldots , \l{n}|F\rangle = C_{a_1}(\l{1})\
C_{a_2}(\l{2})\ldots C_{a_n}(\l{n})\ \vac\  F^{a_n\ldots
a_1}\qquad ,}

\equation{ew}{\tau (\mu)|\l{1},\ldots , \l{n}|F\rangle = \nu
(\mu,\lbrace\l{j}\rbrace,F)\ |\l{1},\ldots , \l{n}|F\rangle }

