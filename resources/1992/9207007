\equation{Lop}{L_n(\l{}) =
\left({\matrix{
a(\l{})-b(\l{})e_n^{11}&-b(\l{})e_n^{21}&b(\l{})e_n^{31}\cr
-b(\l{})e_n^{12}&a(\l{})-b(\l{})e_n^{22}&b(\l{})e_n^{32}\cr
