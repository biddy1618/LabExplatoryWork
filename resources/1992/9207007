\equation{H3again}{\eqalign{H_{(3)}
=i\sum_{j=1}^L\sum_{k=1}^L
&\ -2 \Sd_{j-1}\ S_j\ \Sz_{j+1}
-2 \Sd_{j-1}\ \Sz_j\ S_{j+1}\cr\noalign{\vskip -8pt}
&\ + \Sd_{j-1}\ \qd_{j,1}\ Q_{j+1,-1}
+ \Sd_{j-1}\ Q_{j,-1}\ \qd_{j+1,1}\cr
&\ -2 \Sz_{j-1}\ \Sd_j\ S_{j+1}
+ (1-n_{j,-1})\ Q_{j,-1}\ \qd_{j+1,-1}
+ (1-n_{j,1})\ \qd_{j,1}\ Q_{j+1,1}\cr
&\ -\qd_{j-1,1}\ \Sd_j\ Q_{j+1,-1}
-\qd_{j-1,1}\ (1-n_{j,-1})\ Q_{j+1,1}\cr
&\ +\qd_{j-1,1}\ Q_{j,1}\ (1-n_{j,1})
+\qd_{j-1,1}\ Q_{j,-1}\ \Sd_{j+1}\cr
&\ -\qd_{j-1,-1}\ S_j\ Q_{j+1,1}
-\qd_{j-1,-1}\ (1-n_{j,1})\ Q_{j+1,-1}\cr
&\ +\qd_{j-1,-1}\ Q_{j,-1}\ (1-n_{j,-1})
+\qd_{j-1,-1}\ Q_{j,1}\ S_{j+1}\cr
&\ - h.c.\quad .\cr}}

\equation{H4}{\eqalign{H_{(4)} &= i\lbrack \tilde B, H_{(3)}\rbrack
\cr
&=-2\sum_{k=1}^L K^{\mu\nu}\ K^{\ga\gb}\ K^{\gamma\gd}\
{f_{\gb\gamma}}^\ge\ {f_{\gd\mu}}^\omega J_{k-1,\ga}\ J_{k,\ge}\
J_{k+1,\omega}\ J_{k+2,\nu}\cr
&\qquad +\sum_{k=1}^L { P}^{k-1,k+1}\ -2\sum_{k=1}^L \Pg^{k,k+1}
\qquad ,\cr}}

\equation{P}{{P}^{k-1,k+1} =
\Pg^{k-1,k}\Pg^{k,k+1}\Pg^{k-1,k}\quad .}

\equation{h3}{H_{(3)} = \sum_{n}\sum_{k=1}^L n \lbrack H_{susy}^{n,n+1},
H_{susy}^{k,k+1}\rbrack\quad .}

