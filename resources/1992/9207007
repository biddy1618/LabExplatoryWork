\def\d#1{{}_{#1}}

%*** miscellaneous symbols

\let\del=\nabla
\let\der=\partial
\def\derLR{\rlap{\vbox to1em{%
     \hbox{\m@th$\scriptscriptstyle\leftrightarrow$}\vfil}}\partial}
\def\tr{\mathop{\rm tr}}
\def\cc{{\mathchoice{}{}{\displaystyle*}{\scriptstyle*}}}
\let\hc=\dagger
\def\pdot{{\p3 \cdot\p1}}
\def\fourdx{d^{\p1 4}\n2 x}
\def\sixdx{d^{\p1 6}\n3 x}
\def\ddimdx{d^{\p1 d}\n3 x}
\def\twodth{d^{\p1 2}\n2\theta}
\def\fourdth{d^{\p1 4}\n2\theta}
\def\eightdth{d^{\p1 8}\n2\theta}

\def\Idoubled#1{{\rm I\kern-.22em #1}}
\def\Odoubled#1{{\setbox0=\hbox{\rm#1}%
     \dimen@=\ht0 \dimen@ii=.04em \advance\dimen@ by-\dimen@ii
     \rlap{\kern.26em \vrule height\dimen@ depth-\dimen@ii width.075em}\box0}}
\def\Real{\Idoubled R}
\def\Complex{\Odoubled C}
\def\Quaternion{\Idoubled H}
\def\Octonian{\Odoubled O}

