\tablespace
& \omit && \omit && \omit && \omit && $-{23\over8}$, $-{25\over8}$,
$-{29\over8}$, $-{29\over12}$, $-{79\over24}$, $-{85\over24}$ &\cr
\tablerule
& \omit && \omit && \omit && R && $-{159\over64}$, $-{191\over64}$,
