&& +\{m_{92}R_{deai}R_{fgbc}R_{ajbc}
 +m_{93}R_{ijad}R_{efbc}R_{agbc}
 +m_{94}R_{deai}R_{fjbc}R_{agbc} \nonumber \\
&&\qquad
 +m_{95}R_{debi}R_{afcj}R_{agbc}
